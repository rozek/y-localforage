  "exports": {
    ".": {
      "require":"./dist/y-localforage.umd.js",
      "browser":"./dist/y-localforage.umd.js",
      "import": "./dist/y-localforage.esm.js",
      "svelte": "./dist/y-localforage.esm.js",
      "types":  "./dist/y-localforage.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
