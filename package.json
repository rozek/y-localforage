  },
  "homepage": "https://github.com/rozek/y-localforage#readme",
  "devDependencies": {
    "@types/blueimp-md5": "^2.18.0",
    "agadoo": "^2.0.0",
    "rimraf": "^3.0.2",
    "microbundle": "^0.15.1",
    "tslib": "^2.6.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "lib0": "^0.2.74",
    "localforage": "^1.10.0",
    "yjs": "^13.6.1"
